{
    "manifest_version": 3,
    "name": "current tab tittle extension",
    "description": "Extension which shows the tittle of the current tab",
    "version": "1.0",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "pop.html",
        "default_icon": {
            "16": "images/favicon-16x16.png",
            "32": "images/favicon-32x32.png",
            "192": "images/android-chrome-192x192.png"
        }
    },
    "icons": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "192": "images/android-chrome-192x192.png"
    }
    
}